{
  "name": "hel-tpl-remote-vue-comps",
  "appGroupName": "hel-tpl-remote-vue-comps",
  "main": "hel_proxy/entry.js",
  "module": "hel_proxy_es/entry.js",
  "types": "src/entrance/libTypes.ts",
  "unpkg": "hel_dist/index.html",
  "version": "1.5.1",
  "files": [
    "hel_dist",
    "hel_proxy",
    "hel_proxy_es",
    "scripts",
    "README.md",
    "src"
  ],
  "scripts": {
    "serve": "rsbuild dev",
    "start": "pnpm run start:App",
    "start:App": "cross-env VUE_APP_COMP=App rsbuild dev",
    "start:Hello": "cross-env VUE_APP_COMP=HelloWorld rsbuild dev",
    "build": "pnpm run check_name && pnpm run build_dist && pnpm run build_meta && pnpm run build_proxy",
    "build_dist": "rsbuild build",
    "check_name": "node ./scripts/check.js",
    "build_proxy": "pnpm run build_proxy_umd && pnpm run build_proxy_es && shx cp ./hel_proxy/entry.js ./hel_dist",
    "build_proxy_umd": "rollup -c",
    "build_proxy_es": "cross-env BUILD_ENV=es rollup -c",
    "build_cust": "pnpm run set_home_page \"pnpm run build && pnpm run build_meta\"",
    "set_home_page": "cross-env-shell HEL_APP_HOME_PAGE=http://127.0.0.1:9001",
    "build_meta": "node scripts/meta.js",
    "prepublishOnly": "node ./scripts/prepublishOnly.js"
  },
  "dependencies": {
    "hel-iso": "^4.4.0",
    "hel-lib-proxy": "^4.7.0",
    "vue": "2.7.16"
  },
  "peerDependencies": {
    "hel-lib-proxy": "^4.7.0",
    "vue": "2.7.16"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.17",
    "@rsbuild/plugin-vue2": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "hel-dev-utils": "^4.3.11",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "shx": "^0.3.4",
    "typescript": "4.8.4",
    "vue-template-compiler": "2.7.16",
    "webpack": "^4.47.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
